{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "hide-files.files": [],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#e8cfe3",
        "activityBar.background": "#e8cfe3",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#879748",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#e8cfe3",
        "statusBar.background": "#d8accf",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#c889bb",
        "statusBarItem.remoteBackground": "#d8accf",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#d8accf",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#d8accf99",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#D8ACCF",
    "prettier.documentSelectors": ["**/*.astro"],
    "[astro]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}